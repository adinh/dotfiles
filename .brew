#!/bin/bash

brew update && brew upgrade

formulas=(
  ack
  autoconf
  bash
  chruby
  ctags
  elasticsearch
  ffmpeg
  fontforge
  freetds
  gettext
  git
  "grep --default-names"
  "gnu-sed --default-names"
  gradle
  imagemagick
  ipcalc
  jpeg
  libpng
  libtiff
  mercurial
  mongodb
  mysql
  node
  ngrep
  phantomjs
  pkg-config
  readline
  reattach-to-user-namespace
  ruby-build
  selecta
  sqlite
  unixodbc
  vim
  watch
  "wget --with-iri"
)

for formula in "${formulas[@]}"
do
  brew install $formula
done

brew cleanup --force